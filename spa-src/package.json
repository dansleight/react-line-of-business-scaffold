{
  "name": "line-of-business",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --emptyOutDir --outDir ../src/AiProto/ClientApp/dist",
    "testbuild": "tsc -b && vite build && rm *.tsbuildinfo && rm -rf dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "update-npm": "npm cache verify && npx --yes npm-check-updates --target minor -u && npm update",
    "update-api": "rm -f src/apiClient/*.ts && node generate-api.js"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.5.0",
    "@azure/msal-react": "^3.0.5",
    "@fortawesome/fontawesome-common-types": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@metismenu/react": "^0.0.3",
    "bootstrap": "^5.3.3",
    "bootswatch": "^5.3.3",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "easymde": "^2.19.0",
    "lodash": "^4.17.21",
    "metismenujs": "^1.4.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.5",
    "react-use-cookie": "^1.6.1",
    "simplebar-react": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/bootstrap": "^5.2.10",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.16",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "sass-embedded": "^1.85.0",
    "swagger-typescript-api": "^13.0.23",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0"
  },
  "prettier": {
    "overrides": [
      {
        "files": "routes.tsx",
        "options": {
          "printWidth": 200,
          "arrowParens": "avoid"
        }
      }
    ]
  }
}
